<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor configVersion="3">
    <title>PHP Reader API Documentation</title>
    <paths>
        <output>docs/api</output>
        <cache>docs/.phpdoc-cache</cache>
    </paths>
    <templates>
        <template name="default"/>
    </templates>
    <versions>
        <version number="latest">
            <apis>
                <api>
                    <source dsn=".">
                        <path>src</path>
                    </source>
                    <ignore hidden="true" symlinks="true">
                        <path>vendor</path>
                        <path>tests</path>
                    </ignore>
                </api>
            </apis>
        </version>
    </versions>
</phpdocumentor>

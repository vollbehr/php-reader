{
    "name": "vollbehr/php-reader-laminas",
    "description": "Laminas integration bridge for vollbehr/php-reader.",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/vollbehr/php-reader",
    "keywords": [
        "id3",
        "laminas",
        "metadata",
        "php-reader"
    ],
    "authors": [
        {
            "name": "Sven Vollbehr",
            "email": "sven@vollbehr.io"
        }
    ],
    "support": {
        "issues": "https://github.com/vollbehr/php-reader/issues",
        "source": "https://github.com/vollbehr/php-reader"
    },
    "require": {
        "php": "^8.2",
        "vollbehr/php-reader": "^1.9",
        "psr/container": "^1.1",
        "laminas/laminas-servicemanager": "^3.20",
        "laminas/laminas-modulemanager": "^2.17"
    },
    "autoload": {
        "psr-4": {
            "Vollbehr\\Bridge\\Laminas\\": "src/"
        }
    },
    "extra": {
        "laminas": {
            "component": [
                "Vollbehr\\Bridge\\Laminas\\ConfigProvider"
            ],
            "module": "Vollbehr\\Bridge\\Laminas\\Module"
        }
    }
}
